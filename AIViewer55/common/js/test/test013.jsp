		if(type==CELL_OBJECT){
			out.println("<CellProperty name=\"" + name + "\" property=\"" + property + "\" value=\"" + style + "\"/>");
		}else{
			out.println("<DrawProperty name=\"" + name + "\" property=\"" + property + "\" value=\"" + style + "\"/>");
